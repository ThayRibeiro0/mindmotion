  "bugs": {
    "url": "https://github.com/ThayRibeiro0/mindmotion/issues"
  },
  "homepage": "https://github.com/ThayRibeiro0/mindmotion#readme",
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/chart.js": "^2.9.41",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-chartjs-2": "^2.0.2",
    "@types/react-dom": "^19.1.2",
    "@types/recharts": "^2.0.1",
    "@types/sequelize": "^4.28.20",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.5"
  },
