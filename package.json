{
  "name": "mindmotion",
  "version": "1.0.0",
  "private": true,
  "description": "Mindmotion is a web application that allows you to track your moods and emotions. It provides a simple and intuitive interface for managing your mental health and well-being.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "concurrently \"npm run t\" \"npm run dev:client\"",
    "test:db": "psql -U postgres -d mindmotion_db -c 'SELECT 1;'",
    "build": "tsc",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "dev": "vite --port 10000",
    "dev:server": "node server/src/index.ts",
    "dev:client": "cd client && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThayRibeiro0/mindmotion.git"
  },
