{
  "name": "mindmotion",
  "version": "1.0.0",
  "private": true,
  "description": "Mindmotion is a web application that allows you to track your moods and emotions. It provides a simple and intuitive interface for managing your mental health and well-being.",
  "main": "index.js",
  "workspaces": [
    "client",
    "server"
  ],
  "type": "module",
  "scripts": {
    "start": "ts-node server/src/index.ts",
    "start:dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev-server": "nodemon src/index.ts",
    "dev:client": "cd client && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThayRibeiro0/mindmotion.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThayRibeiro0/mindmotion/issues"
  },
  "homepage": "https://github.com/ThayRibeiro0/mindmotion#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/sequelize": "^4.28.20",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  }
}
