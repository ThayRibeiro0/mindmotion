{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@models/*": ["src/models/*"],
      "@routes/*": ["src/routes/*"]
    },
    "jsx": "react-jsx",
    "outDir": "../dist/server", 
    "rootDir": "./",
    "module": "NodeNext",
    "target": "ES2020",
    "moduleResolution": "NodeNext",
  },
  "include": ["src"],
  "typeRoots": ["./node_modules/@types", "./src/types"]
}
